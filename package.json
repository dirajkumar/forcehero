    "cross-env": "^6.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsforce": "^1.9.3",
    "lodash": "^4.17.19",
    "nuxt": "^2.11.0",
    "query-string": "^6.9.0",
    "vue-animejs": "^2.1.1",
